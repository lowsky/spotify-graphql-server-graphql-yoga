    "@whatwg-node/fetch": "0.8.4",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "graphql": "16.6.0",
    "graphql-yoga": "3.8.0",
    "sass": "1.62.0",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/node": "18.16.5",
    "@vercel/node": "2.11.0",
    "tsx": "3.12.6"
