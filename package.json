    "start": "npm run build ; tsx -r dotenv/config serverInit.ts"
  },
  "dependencies": {
    "@envelop/core": "5.4.0",
    "@graphql-tools/mock": "9.0.25",
    "@graphql-yoga/plugin-response-cache": "3.18.2",
    "@whatwg-node/fetch": "0.10.13",
    "cors": "2.8.5",
    "dataloader": "2.2.3",
    "dotenv": "17.2.3",
    "express": "5.1.0",
    "graphql": "16.12.0",
    "graphql-yoga": "5.16.0",
    "sass": "1.94.0",
    "typescript": "5.9.3"
  },
  "devDependencies": {
    "@types/cors": "2.8.19",
    "@types/express": "5.0.3",
    "@types/node": "24.10.1",
    "@vercel/node": "5.4.1",
    "tsx": "4.20.6"
  }
