    "start": "npm run build ; tsx -r dotenv/config serverInit.ts"
  },
  "dependencies": {
    "@envelop/core": "5.2.0",
    "@graphql-tools/mock": "9.0.20",
    "@graphql-yoga/plugin-response-cache": "3.14.0",
    "@whatwg-node/fetch": "0.10.3",
    "cors": "2.8.5",
    "dataloader": "2.2.3",
    "dotenv": "16.4.7",
    "express": "4.21.2",
    "graphql": "16.10.0",
    "graphql-yoga": "5.12.0",
    "sass": "1.85.1",
    "typescript": "5.7.3"
  },
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "5.0.0",
    "@types/node": "22.13.9",
    "@vercel/node": "5.1.11",
    "tsx": "4.19.3"
  }
