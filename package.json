    "@graphql-yoga/plugin-response-cache": "3.15.6",
    "@whatwg-node/fetch": "0.10.8",
    "cors": "2.8.5",
    "dataloader": "2.2.3",
    "dotenv": "16.5.0",
    "express": "5.1.0",
    "graphql": "16.11.0",
    "graphql-yoga": "5.13.5",
    "sass": "1.89.1",
    "typescript": "5.8.3"
  },
  "devDependencies": {
    "@types/cors": "2.8.18",
    "@types/express": "5.0.2",
    "@types/node": "22.15.29",
    "@vercel/node": "5.1.16",
    "tsx": "4.19.4"
  }
