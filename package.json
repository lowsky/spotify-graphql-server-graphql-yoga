    "@whatwg-node/fetch": "0.10.13",
    "cors": "2.8.6",
    "dataloader": "2.2.3",
    "dotenv": "17.2.3",
    "express": "5.2.1",
    "graphql": "16.12.0",
    "graphql-yoga": "5.18.0",
    "sass": "1.97.3",
    "typescript": "5.9.3"
  },
  "devDependencies": {
    "@types/cors": "2.8.19",
    "@types/express": "5.0.6",
    "@types/node": "24.10.13",
    "@vercel/node": "5.5.28",
    "tsx": "4.20.6"
