    "@whatwg-node/fetch": "0.9.6",
    "cors": "2.8.5",
    "dotenv": "16.1.4",
    "express": "4.18.2",
    "graphql": "16.6.0",
    "graphql-yoga": "4.0.0",
    "sass": "1.63.6",
    "typescript": "5.1.3"
  },
  "devDependencies": {
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/node": "18.16.18",
    "@vercel/node": "2.15.1",
    "tsx": "3.12.7"
